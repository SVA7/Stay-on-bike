  pod 'RaisePlaceholder'
  pod 'UXCam'
  pod 'Kingfisher'
  pod 'SwiftyOnboard'
  pod 'SwiftNotificationCenter'

  # Pods for Stay on bike

  target 'Stay on bikeTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Stay on bikeUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
